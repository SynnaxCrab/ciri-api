{
  "name": "ciri-api",
  "version": "0.0.1",
  "description": "Ciri API",
  "main": "index.js",
  "engines": {
    "node": "9.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "start": "node app.js",
    "dist": "node dist/app.js",
    "dev": "nodemon index.js",
    "test": "mocha --recursive --compilers js:babel-register",
    "migrate:dev": "sequelize --config src/config/database.js db:migrate",
    "rollback:dev": "sequelize --config src/config/database.js db:migrate:undo",
    "migrate:prod": "sequelize --config config/database.js db:migrate",
    "rollback:prod": "sequelize --config config/database.js db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "faker": "^3.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rewire": "^2.5.2",
    "rosie": "^1.6.0"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "graphql": "^0.11.7",
    "apollo-server-koa": "^1.2.0",
    "graphql-tools": "^2.8.0",
    "jsonapi-serializer": "^3.4.1",
    "kcors": "2.2.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.3.0",
    "pg": "^7.4.0",
    "sequelize": "^4.22.11",
    "sequelize-cli": "^3.1.0"
  }
}
